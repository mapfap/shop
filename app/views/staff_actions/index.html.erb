<h1>Listing staff_actions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @staff_actions.each do |staff_action| %>
      <tr>
        <td><%= link_to 'Show', staff_action %></td>
        <td><%= link_to 'Edit', edit_staff_action_path(staff_action) %></td>
        <td><%= link_to 'Destroy', staff_action, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Staff action', new_staff_action_path %>
